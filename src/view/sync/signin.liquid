{% include "front/inc/header" %}

  <div class="container" style="margin-top:100px;">
    <div class="row">
      <div class="col-md-4 col-md-offset-4">
        <div class="signin-panel panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">Sign In Succeed!</h3>
          </div>
          <div class="panel-body">
            <a href="{{app_data.s|escape}}">Redirect to {{app_name|app_get_title_by_name|escape}}, please wait...</a>
          {% for item in sync_list %}
            <script src="{{item|escape}}"></script>
          {% endfor %}
            <script>
              setTimeout(function () {
                location.replace('{{app_data.s|escape}}');
              }, 1000);
            </script>
          </div>
        </div>
      </div>
    </div>
  </div>

{% include "front/inc/footer" %}
